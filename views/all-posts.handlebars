<div class="posts">
    {{#each posts}}
    <article class="card">
        <div class="flex-row">
            <h2>
                <a href="/post/{{id}}">{{title}}</a>
            </h2>
        <div>
            Posted by: {{User.username}}
        </div>
        </div>
        <div>
            {{body}}
        </div>

    </article>
    {{/each}}
</div>